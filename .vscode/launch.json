{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: evaluate.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/evaluate.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}",
            "args": [
                "--model_id",
                "gpt-4o-mini",
                "--num_samples",
                "3",
                "--verbose"
            ]
        }
    ]
}
